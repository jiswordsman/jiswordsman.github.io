<!--SEO-->
<% if(config.description && page.description){ %>
<meta name="description" content="<%= config.description%> | <%= page.description%>"/>
<% } %>
<% if (config.description && !page.description){ %>
<meta name="description" content="<%= config.description%>"/>
<% } %>
<% if (!config.description && page.description){ %>
<meta name="description" content="<%= page.description%>"/>
<% } %>

<% if(config.keywords && page.keywords){ %>
<meta name="keywords" content="<%= config.keywords%> ,<%= page.keywords%>"/>
<% } %>
<% if (config.keywords && !page.keywords){ %>
<meta name="keywords" content="<%= config.keywords%>"/>
<% } %>
<% if (!config.keywords && page.keywords){ %>
<meta name="keywords" content="<%= page.keywords%>"/>
<% } %>

<% if(theme.baidu_ssp_verify){ %>
<meta name="baidu_ssp_verify" content="<%= theme.baidu_ssp_verify%>">
<% } %>
<% if(theme.zhuchao_union_verify){ %>
<meta name="zhuchao_union_verify" content="<%= theme.zhuchao_union_verify%>">
<% } %>
<% if(theme.sll_ssp_verify){ %>
<meta name="360_ssp_verify" content="<%= theme.sll_ssp_verify%>">
<% } %>

<% var robotMeta = theme.robot||'all'; %>
<meta name="robots" content="<%= robotMeta%>" />
<meta name="google" content="<%= robotMeta%>" />
<meta name="googlebot" content="<%= robotMeta%>" />
<meta name="verify" content="<%= robotMeta%>" />

<script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
<script>
    (adsbygoogle = window.adsbygoogle || []).push({
        google_ad_client: "ca-pub-1672129391773476",
        enable_page_level_ads: true
    });
</script>